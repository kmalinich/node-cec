{
	"name": "node-cec",
	"version": "0.1.3",
	"description": "cec-client wrapper",
	"author": "Patrick Wozniak <hello@patwoz.de>",
	"main": "index.js",
	"keywords": [
		"cec",
		"hdmi"
	],
	"repository": {
		"type": "git",
		"url": "git://github.com/kmalinich/node-cec.git"
	},
	"license": "MIT",
	"coffeelintConfig": {
		"indentation": {
			"level": "error",
			"value": 2
		},
		"line_endings": {
			"value": "unix",
			"level": "error"
		},
		"max_line_length": {
			"level": "warn"
		},
		"no_trailing_whitespace": {
			"level": "warn"
		}
	}
}
